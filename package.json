{
  "name": "svelte-parcel-talwind-starter",
  "version": "1.0.0",
  "description": "Svelte with Tailwind and Parcel",
  "main": "./src/main.js",
  "author": "iljoo",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "parcel src/index.html --port 3000",
    "dev": "cross-env NODE_ENV=development npm-run-all --parallel dev:*",
    "dev:svelte": "npm run start",
    "build": "cross-env NODE_ENV=production npm-run-all --parallel build:*",
    "build:svelte": "rm -rf dist && parcel build src/index.html --no-source-maps",
    "convert": "synp --source-file yarn.lock -f",
    "check-types": "tsc",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.svelte .",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|ts|svelte|json)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "validate": "npm-run-all --parallel check-types check-format lint build",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:e2e": "start-server-and-test start http://localhost:5020 cy:run",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "cy": "is-ci \"test:e2e\" \"cy:open\"",
    "cy:open": "cypress open",
    "cy:run": "cross-env CYPRESS_BASE_URL=http://localhost:5020 cypress run",
    "upgrade-interactive": "npm-check --update"
  },
  "browserslist": [
    "last 1 chrome versions"
  ],
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.1.0",
    "autoprefixer": "^9.7.3",
    "cross-env": "^7.0.2",
    "eslint": "^7.5.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "husky": "^4.2.5",
    "is-ci-cli": "^2.1.2",
    "lint-staged": "^10.2.11",
    "npm-check": "^5.9.2",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-svelte": "^4.0.5",
    "postcss-nested": "^4.2.3",
    "prettier": "^2.0.5",
    "prettier-plugin-svelte": "^0.7.0",
    "svelte": "^3.16.7",
    "tailwindcss": "^1.1.4",
    "tailwindcss-font-inter": "^1.0.7"
  },
  "dependencies": {}
}
